{
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.cursorBlinking": "solid",
  "editor.cursorSurroundingLines": 5,
  "editor.fontFamily": "FiraCode Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontSize": 19,
  "editor.formatOnSave": true,
  "editor.lineHeight": 24,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.parameterHints.cycle": true,
  "extensions.ignoreRecommendations": true,
  "files.enableTrash": false,
  "files.hotExit": "off",
  "files.insertFinalNewline": true,
  "git.allowForcePush": true,
  "haskell.plugin.ghcide-completions.config.snippetsOn": false,
  "python.formatting.provider": "black",
  "python.linting.pylintEnabled": true,
  "security.workspace.trust.enabled": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.defaultProfile.linux": "fish",
  "typescript.surveys.enabled": false,
  "update.mode": "none",
  "vim.hlsearch": true,
  "vim.leader": "<Space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["Y"],
      "after": ["y", "$"]
    },
    {
      "before": ["<Tab>"],
      "after": ["C-^"]
    },
    {
      "before": ["[", "<Space>"],
      "after": ["O", "<Esc>", "j", "0"]
    },
    {
      "before": ["]", "<Space>"],
      "after": ["o", "<Esc>", "k", "0"]
    },
    {
      "before": ["[", "b"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["]", "b"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["[", "c"],
      "commands": [
        "workbench.action.editor.previousChange",
        "workbench.action.compareEditor.previousChange"
      ]
    },
    {
      "before": ["]", "c"],
      "commands": [
        "workbench.action.editor.nextChange",
        "workbench.action.compareEditor.nextChange"
      ]
    },
    {
      "before": ["[", "e"],
      "commands": ["editor.action.moveLinesUpAction"]
    },
    {
      "before": ["]", "e"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["[", "q"],
      "commands": ["search.action.focusPreviousSearchResult"]
    },
    {
      "before": ["]", "q"],
      "commands": ["search.action.focusNextSearchResult"]
    },
    {
      "before": ["<Leader>", "<Leader>"],
      "commands": [":nohl"]
    },
    {
      "before": ["<Leader>", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<Leader>", "/"],
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["<Leader>", "*"],
      "after": ["v", "i", "w"],
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["<C-H>"],
      "after": ["<C-W>", "h"],
      "commands": ["workbench.action.compareEditor.focusSecondarySide"]
    },
    {
      "before": ["<C-J>"],
      "after": ["<C-W>", "j"]
    },
    {
      "before": ["<C-K>"],
      "after": ["<C-W>", "k"]
    },
    {
      "before": ["<C-L>"],
      "after": ["<C-W>", "l"],
      "commands": ["workbench.action.compareEditor.focusPrimarySide"]
    }
  ],
  "vim.replaceWithRegister": true,
  "vim.smartcase": false,
  "vim.startofline": false,
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["[", "e"],
      "commands": ["editor.action.moveLinesUpAction"]
    },
    {
      "before": ["]", "e"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["<Leader>", "*"],
      "commands": ["workbench.action.findInFiles"]
    }
  ],
  "vim.visualstar": true,
  "workbench.colorCustomizations": {
    "[GitHub Light Default]": {
      "editorCursor.background": "#ffffff",
      "editorCursor.foreground": "#044289",
      "terminalCursor.background": "#ffffff",
      "terminalCursor.foreground": "#044289"
    }
  },
  "workbench.colorTheme": "GitHub Light Default",
  "workbench.enableExperiments": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "[cabal]": {
    "editor.tabSize": 2
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[haskell]": {
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[shellscript]": {
    "editor.tabSize": 2
  },
  "[toml]": {
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[xml]": {
    "editor.tabSize": 2
  }
}
